{
  "name": "jira-hook-queue-manager",
  "version": "1.0.0",
  "description": "Jira webhooks queue manager",
  "main": "config.example.js",
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "lint": "eslint \"{src,test}/**/*.js\"",
    "start": "node src/index.js",
    "start-dev": "nodemon src/index.js",
    "debug": "nodemon --inspect src/index.js",
    "test-single": "NODE_ENV=test nyc mocha",
    "test": "NODE_ENV=test nyc mocha test/* --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixassio/jira-hook-queue-manager.git"
  },
  "nyc": {
    "exclude": [
      "src/redis-client.js",
      "src/modules/log.js",
      "src/config",
      "src/timeline-handler/commands/help.js",
      "test"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mixassio/jira-hook-queue-manager/issues"
  },
  "homepage": "https://github.com/mixassio/jira-hook-queue-manager#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "delay": "^4.3.0",
    "express": "^4.17.1",
    "html-to-text": "^5.1.1",
    "marked": "^0.8.0",
    "node-polyglot": "^2.4.0",
    "ramda": "^0.27.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "util": "^0.12.2",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "latest",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "mocha": "latest",
    "nock": "^11.4.0",
    "nodemon": "^1.19.4",
    "nyc": "^15.0.0",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2"
  }
}
